 (function (bannerCode) {
 var bannerInfo = window.banners[bannerCode];
 if (!bannerInfo) {
 try {
 window.errorReporter.report(new Error('ADS_LOGGER: banner ' + bannerCode + ' is undefined'));
 } catch (_) {
 }
 return;
 }
 var adfoxPageId = window.banners.isYandexTestId ? bannerInfo.adfoxTestPageId : bannerInfo.adfoxPageId;
  if (!window.Ya || !window.Ya.adfoxCode) {
 if (window.avito.ads.showDfp) {
 window.avito.ads.showDfp(bannerCode, false);
